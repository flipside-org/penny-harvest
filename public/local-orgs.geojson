---
permalink: json/local-orgs.geojson
---
{ "type": "FeatureCollection",
  "features": [
    {% for org in site.categories.local_org %}
    { "type": "Feature",
      "geometry": {
        "type": "Point",
        "coordinates": [{{ org.lng }}, {{ org.lat }}]
      },
      "properties": {
        "title": "{{ org.title }}",
        "url": "{{ org.url }}",
        "impact_area": "{{ site.data.impact_areas[org.impact_area].id }}"
      }
    }{% unless forloop.last %},{% endunless %}{% endfor %}
  ]
}