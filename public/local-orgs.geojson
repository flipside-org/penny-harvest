---
permalink: json/local-orgs.geojson
---
{ "type": "FeatureCollection",
  "features": [
    {% for org in site.categories.local_org %}
    { "type": "Feature",
      "geometry": {
        "type": "Point",
        "coordinates": [{{ org.lat }}, {{ org.lng }}]
      },
      "properties": {
        "title": "{{ org.title }}",
        "url": "{{ org.url }}",
        "impact_area": {
          "name" : "{{ org.impact_area }}",
          "id" : "{{ site.data.impact_areas[org.impact_area].id }}",
          "class" : "{{ site.data.impact_areas[org.impact_area].class }}"
        },
        "keywords" : []
      }
    }{% unless forloop.last %},{% endunless %}{% endfor %}
  ]
}